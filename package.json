{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "default": "src/index.ts",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "webpack --mode=production --node-env=production",
    "start": "npm run build && node ./server.js",
    "eslint": "npx eslint . --ext .js,.ts ",
    "eslint fix": "npx eslint --fix --fix-type suggestion --fix-type problem,layout .",
    "stylelint": "npx stylelint '**/*.css' ",
    "check": "tsc --noEmit",
    "test": "mocha --require ts-node/register --require ./mochaSetup.js './src/**/*.test.ts'",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KosilovaDaria/middle.messenger.praktikum.yandex.git"
  },
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "distDir": "dist"
    }
  },
  "author": "Kosilova DD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KosilovaDaria/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/KosilovaDaria/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "2.7.0",
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "10.0.13",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@webpack-cli/generators": "2.5.0",
    "autoprefixer": "10.4.7",
    "chai": "4.3.6",
    "css-loader": "6.6.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "express-history-api-fallback": "2.2.1",
    "handlebars-loader": "1.7.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jsdom": "20.0.1",
    "mocha": "10.0.0",
    "parcel": "2.7.0",
    "parcel-transformer-hbs": "^1.0.4",
    "postcss": "8.4.16",
    "postcss-loader": "6.2.1",
    "proxyquire": "2.1.3",
    "sinon": "14.0.1",
    "style-loader": "3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "@parcel/transformer-css": "^2.6.2",
    "express": "4.18.1",
    "handlebars": "^4.7.7",
    "postcss-modules": "^4.3.1",
    "uuid": "^8.3.2"
  }
}
