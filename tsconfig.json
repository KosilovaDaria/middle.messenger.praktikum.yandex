{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2017",
    "lib": ["es2017"],
    "declaration": false,
    "module": "ES6",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  }
}